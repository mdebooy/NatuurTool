{
  "_jar": "NatuurTools.jar",
  "_prefixlengte": 20,
  "applicatie": "TaxaImport",
  "help": "Importeren van taxastructuur, in JSON formaat, met namen in verschillende talen in de database.",
  "banner": "JSON bestand met taxa importeren",
  "parameters": [
    {
      "parameter": "aanmaak",
      "lang": "aanmaak",
      "help": "De ontbrekende taxa toevoegen.",
      "type": "boolean"
    },
    {
      "parameter": "charsetin",
      "lang": "charsetin",
      "help": "De characterset van de invoer als deze niet {0} is.",
      "type": "charset"
    },
    {
      "parameter": "dburl",
      "lang": "dburl",
      "help": "De URL voor de database connectie (localhost:5432/db).",
      "verplicht": true
    },
    {
      "parameter": "dbuser",
      "lang": "dbuser",
      "help": "De database gebruikersnaam.",
      "verplicht": true
    },
    {
      "parameter": "hernummer",
      "lang": "hernummer",
      "help": "Hernummer de taxa.",
      "type": "boolean"
    },
    {
      "parameter": "json",
      "extensie": "json",
      "kort": "j",
      "lang": "jsonbestand",
      "help": "IOC Namen bestand in JSON-formaat.",
      "type": "bestand",
      "verplicht": true
    },
    {
      "parameter": "logging",
      "extensie": "log",
      "kort": "l",
      "lang": "logbestand",
      "help": "Bestand met de verwerkte taxa uit het <jsonbestand>.",
      "type": "bestand"
    },
    {
      "parameter": "metondersoort",
      "lang": "metondersoort",
      "help": "Ook de ontbrekende ondersoorten toevoegen.",
      "type": "boolean"
    },
    {
      "parameter": "readonly",
      "lang": "readonly",
      "help": "Enkel lezen. Nu worden <aanmaken>, <hernummer> en <skipstructuur> genegeerd.",
      "type": "boolean"
    },
    {
      "parameter": "stil",
      "lang": "stil",
      "help": "Minimale uitvoer op het scherm.",
      "type": "boolean"
    },
    {
      "parameter": "taal",
      "lang": "taal",
      "help": "Taal van de namen, die naast de latijnse naam staan, als die anders is dan de taal van het systeem ({0}).",
      "type": "locale"
    },
    {
      "parameter": "talen",
      "lang": "talen",
      "help": "Lijst, door komma''s gescheiden, met talen die in het overzicht mogen staan."
    },
    {
      "parameter": "wachtwoord",
      "kort": "w",
      "lang": "wachtwoord",
      "help": "Het wachtwoord van de database gebruiker."
    }
  ]
}
